{
  "manifest_version": 2,

  "name": "SnipHub",
  "description": "This extension lets you create your own snippets",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "downloads"
    // "https://sniphub.herokuapp.com/*"
  ]
}
